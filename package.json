{
  "name": "bed-assignment-2",
  "version": "0.0.0",
  "private": true,
  "description": "Submission for school's backend web development (BED) assignment",
  "homepage": "https://github.com/cNoside/bed-assignment-2",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "start": "turbo run start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "cm": "custom-commit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "prettier-config": "*",
    "scripts": "*",
    "prettier": "latest",
    "turbo": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "husky": "^8.0.0"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "npm@8.11.0"
}